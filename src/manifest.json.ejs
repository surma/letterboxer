{
  "short_name": "Letterboxer",
  "name": "Letterboxer",
  "description": "Letterboxes your images",
  "icons": [
    {
      "src": "<%= Object.values(bundle).find(chunk => /\/logo-[^/]+$/.test(chunk.fileName)).fileName; %>",
      "type": "image/png",
      "sizes": "512x512"
    }
  ],
  "start_url": "/",
  "background_color": "#ddd",
  "display": "standalone",
  "theme_color": "#222",
  "share_target": {
    "action": "/share-target",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "files": [
        {
          "name": "file",
          "accept": ["image/*"]
        }
      ]
    }
  }
}